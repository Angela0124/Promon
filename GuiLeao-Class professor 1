    public String getNome() {
        return nome;
    }

    public int getMatricula() {
        return matricula;
    }

    public int getCpf() {
        return cpf;
    }

    public int getRg() {
        return Rg;
    }


    
    public void mostrar_professor(){
        System.out.println(nome);
    }
    public void dados_professor(){
        System.out.println( nome);
        System.out.println(matricula );
        System.out.println(cpf);
        System.out.println(Rg);
    }
   
    public Professor Cadastrar_prof(Professor professor) {
        return professor;
        
    }

   public boolean cadastrar_projeto (){
       if (cadastrar==true){
           System.out.println("Cadastro do projeto foi efetuado com sucesso");}
       else{
           System.out.println("O projeto n√£o foi cadastrado");
       }
    return cadastrar;
                 
   }
    
