package model.bean;

//esse tipo de classe que representa uma entidade é comumente chamada de "bean"
//no lugar de Categoria deve-se criar as classes apresentadas no Banco de dados com seus atributos. 

public class ClasseBean {
    private int id;
    private String descricao;
    
    //construtor com descrição

    /**
     *
     * @param descricao
     */
    public ClasseBean(String descricao) {
        this.descricao = descricao;
    }
    
    //construtor vazio

    /**
     *
     */
    public ClasseBean() {
    }

    //getters e setters
    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getDescricao() {
        return descricao;
    }

    public void setDescricao(String descricao) {
        this.descricao = descricao;
    }
}
